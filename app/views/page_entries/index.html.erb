<h1>Listing page_entries</h1>

<table>
  <tr>
    <th>Page</th>
    <th>Block</th>
    <th>Feed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @page_entries.each do |page_entry| %>
  <tr>
    <td><%= page_entry.page %></td>
    <td><%= page_entry.block %></td>
    <td><%= page_entry.feed %></td>
    <td><%= link_to 'Show', page_entry %></td>
    <td><%= link_to 'Edit', edit_page_entry_path(page_entry) %></td>
    <td><%= link_to 'Destroy', page_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Page entry', new_page_entry_path %>
